{
    "name": "pilopress-starter-theme",
    "version": "1.0.0",
    "description": "Blank starter theme ready to use for Pilo'Press.",
    "main": "postcss.config.js",
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@babel/register": "^7.12.13",
        "babel-loader": "^8.2.2",
        "browser-sync": "^2.26.14",
        "del": "^6.0.0",
        "fibers": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "autoprefixer": "^10.2.4",
        "cssnano": "^4.1.10",
        "postcss": "^8.2.4",
        "postcss-cli": "^8.3.1",
        "postcss-nested": "^5.0.3",
        "postcss-prefix-selector": "^1.7.2",
        "gulp-newer": "^1.4.0",
        "gulp-postcss": "^9.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-uglify": "^3.0.2",
        "gulp-util": "^3.0.8",
        "tailwindcss": "^2.0.2",
        "@tailwindcss/forms": "^0.2.1"
    },
    "scripts": {
        "pre-install": "npm install",
        "build-admin": "cd pilopress/assets && postcss ./styles.css -o ./styles-admin.min.css --config ./admin-config --verbose",
        "build-front": "cd pilopress/assets && postcss ./styles.css -o ./styles.min.css --config ./front-config --verbose",
        "build": "npm run build-front && npm run build-admin"
    },
