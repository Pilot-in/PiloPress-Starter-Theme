{
    "name": "pilopress-starter-theme",
    "version": "1.0.0",
    "description": "This theme is a blank starter theme.",
    "main": "postcss.config.js",
    "dependencies": {
        "autoprefixer": "^10.2.1",
        "cssnano": "^4.1.10",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "postcss-import": "^14.0.2",
        "postcss-nesting": "^10.1.1",
        "postcss-prefix-selector": "^1.7.2",
        "postcss-preset-env": "^7.2.0",
        "tailwindcss": "^3.0.15"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.2.1"
    },
    "scripts": {
        "pre-install": "npm install",
        "watch-admin": "npx tailwindcss -i ./pilopress/assets/styles.css -o ./pilopress/assets/styles-admin.min.css -c ./tailwind.config.js --postcss ./pilopress/assets/admin-config/postcss.config.js --minify --watch",
        "watch-front": "npx tailwindcss -i ./pilopress/assets/styles.css -o ./pilopress/assets/styles.min.css -c ./tailwind.config.js --postcss ./pilopress/assets/front-config/postcss.config.js --minify --watch",
        "build-admin": "npx tailwindcss -i ./pilopress/assets/styles.css -o ./pilopress/assets/styles-admin.min.css -c ./tailwind.config.js --postcss ./pilopress/assets/admin-config/postcss.config.js --minify",
        "build-front": "npx tailwindcss -i ./pilopress/assets/styles.css -o ./pilopress/assets/styles.min.css -c ./tailwind.config.js --minify",
        "build": "npm run build-front && npm run build-admin",
        "watch": "npm run watch-front && npm run watch-admin"
    },
    "author": "Pilot-in",
    "license": "ISC"
}
