    "description": "Blank starter theme ready-to-use for Pilo'Press.",
    "main": "./pilopress/assets/gulpfile.babel.js",
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "browserslist": [
        "defaults"
    ],
    "dependencies": {
        "@tailwindcss/forms": "^0.2.1",
        "tailwindcss": "^2.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@babel/register": "^7.12.13",
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "browser-sync": "^2.26.14",
        "cssnano": "^4.1.10",
        "del": "^6.0.0",
        "fibers": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-newer": "^1.4.0",
